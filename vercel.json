{
  "version": 2,
  "builds": [
    {
      "src": "rsn-frontend/package.json",
      "use": "@vercel/angular"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/rsn-frontend/$1"
    }
  ],
  "buildCommand": "cd rsn-frontend && npm install && npm run build -- --output-path=dist/rsn-frontend/browser",
  "outputDirectory": "rsn-frontend/dist/rsn-frontend/browser"
}